{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "outDir": "./dist",
        "strict": true,
        "baseUrl": "./",
        "typeRoots": [
            "node_modules/@types"
        ],
        "types": [
            "node", "jest"
        ],
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "inlineSourceMap": true
    },
    "exclude": [
      "node_modules", "prisma/*", "./coverage", "./dist", "tests", "jest.config.js,", "microfrontend"
    ]
}